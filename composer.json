{
    "name": "kidshenlong/cloud-cloud-lumen",
    "description": "Comic Cloud Lumen",
    "keywords": ["lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "lucadegasperi/oauth2-server-laravel": "^5.0",
        "doctrine/dbal": "^2.5",
        "nesbot/carbon": "^1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0",
        "php-vcr/php-vcr": "^1.2",
        "php-vcr/phpunit-testlistener-vcr": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/",
            "tests/api/ApiTester.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
